{
  "bot-name": "PATERSON",
  "description": "WhatsApp Bot Created by Kervens AUBOURG",
  "logo": "https://i.ibb.co/Mx4v92Dr/malvin-xd.jpg",
  "keywords": ["whatsapp-bot", "automation", "nodejs"],
  "documentation": "https://your-docs-url.com",
  "support": "contact@yourdomain.com",
  
  "env": {
    "SESSION_ID": {
      "description": "Your unique session identifier (should start with 'paterson-')",
      "value": "paterson-",
      "required": true,
      "pattern": "^paterson-.+",
      "secure": true
    },
    "PREFIX": {
      "description": "Command prefix (single character recommended)",
      "value": ".",
      "required": true,
      "validation": "^[!@#%^&*.,;:]{1}$"
    },
    "OWNER_NAME": {
      "description": "Bot maintainer's name",
      "value": "Kervens AUBOURG",
      "required": false
    },
    "OWNER_NUMBER": {
      "description": "Owner's WhatsApp number (with country code)",
      "value": "50942737567",
      "required": true,
      "validation": "^\\d{8,15}$"
    },
    "MODE": {
      "description": "Bot accessibility mode",
      "value": "private",
      "required": true,
      "options": ["private", "public", "group-only"]
    },
    "SECURITY_KEY": {
      "description": "Encryption key for sensitive data",
      "required": true,
      "secure": true,
      "generator": "uuidv4"
    }
  },
  "meta": {
    "version": "2.0.0",
    "license": "MIT",
    "repository": "https://github.com/PATERSON-MD/PATERSON"
  }
}
