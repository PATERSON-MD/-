{
  "name": "PATERSON",
  "description": "WhatsApp bot created by Kervens AUBOURG",
  "logo": "https://i.ibb.co/Mx4v92Dr/malvin-xd.jpg",
  "keywords": ["PATERSON", "whatsapp-bot"],
  "success_url": "/",
  "stack": "container",
  
  "env": {
    "SESSION_ID": {
      "description": "Your WhatsApp session ID",
      "required": true,
      "value": ""
    },
    
    "STICKER_NAME": {
      "description": "Sticker pack name",
      "required": false,
      "value": "PATERSON"
    },
    
    "PREFIX": {
      "description": "Bot command prefix",
      "required": false,
      "value": "."
    },
    
    "MODE": {
      "description": "Bot mode (public/private/inbox/group)",
      "required": false,
      "value": "public"
    },
    
    "ALWAYS_ONLINE": {
      "description": "Keep bot always online",
      "required": false,
      "value": "false"
    },
    
    "BOT_NAME": {
      "description": "Display name for your bot",
      "required": false,
      "value": "PATERSON"
    },
    
    "OWNER_NAME": {
      "description": "Bot owner's name",
      "required": false,
      "value": "Kervens AUBOURG"
    },
    
    "OWNER_NUMBER": {
      "description": "Bot owner's WhatsApp number",
      "required": false,
      "value": "50942737567"
    },
    
    "AUTO_STATUS_MSG": {
      "description": "Auto-reply message for status updates",
      "required": false,
      "value": "*SEEN YOUR STATUS BY PATERSON*"
    },
    
    "DESCRIPTION": {
      "description": "Bot description",
      "required": false,
      "value": "Powered by Kervens AUBOURG"
    },
    
    // [Keep all other environment variables unchanged...]
  },
  
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  
  "stack": "heroku-24"
}
